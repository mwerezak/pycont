{
  "io": {
      "type": "serial",
      "port": "/dev/ttyUSB0",
      "baudrate": 9600,
      "timeout": 1
  },
  "default": {
    "model": "C3000",
    "volume": 5,
    "micro_step_mode": 2,
    "top_velocity": 24000,
    "initialize_valve_position": "I"
  },
  "groups": {
    "chemicals": ["acetone", "water"]
  },
  "pumps": {
      "acetone": {
        "switch": "0"
      },
      "water": {
          "switch": "1",
          "top_velocity": 12000
      }
  }
}
